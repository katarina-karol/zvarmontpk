/**************** menu *******************/

#menu ul
{
	padding: 0px 0px 0px 20px;
	margin: 0px 0px 0px 0px;
	text-align: left;
	list-style:none;
}

#menu ul li
{
	padding:0px 0px 0px 0px;
	margin:0px 0px 0px 0px;	
	background: url(../images/menu_li.jpg) top left no-repeat;
	float:left;
}

#menu ul li a.selected
{
	text-decoration: underline;	
	color: white;
}

#menu ul li a
{
	display:block;
	text-decoration: none;
	padding:0px 0px 0px 0px;
	margin: 0px;
	color: #929292;
	font-weight:bold;
	height:51px;
	float:left;
	background: url(../images/menu_item.jpg) top right no-repeat;
}

#menu ul li a:hover
{
	text-decoration: underline;	
	color: white;
}

#menu ul li a span
{
    display:block;
    padding:17px 10px 0px 10px;
}

/**************** menu_sub *******************/

div#menu_sub UL
{
	list-style: none;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	width:100%;
}

div#menu_sub UL LI
{
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;
	display:block;
	background: url(../images/menu_sub_item.jpg) top left no-repeat;
}

div#menu_sub A.selected
