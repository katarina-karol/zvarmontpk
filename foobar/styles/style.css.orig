	color:white;
}

div#menu_sub A
{
	display:block;
	text-decoration: none;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;
	font-size:1.0em;
	_height:20px;
	min-height:20px;
	font-weight:bold;
	color: #636363;
	background: url(../images/menu_sub_item2.jpg) bottom left no-repeat;
}

div#menu_sub A span
