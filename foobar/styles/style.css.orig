	display: block;
	padding: 3px 5px 3px 10px;
	margin: 0px 0px 0px 0px;
}

div#menu_sub A:visited
{
}

div#menu_sub A:hover
{
	text-decoration: none;
	background: #636363 url(../images/menu_sub_hover.jpg) top left repeat-y;
	color:white;
}

