BODY
{
	margin: 0;
	padding: 0;
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-weight: normal;
	font-size: 0.7em;
	text-align: center;
	background: #383838 url(../images/bg.jpg) top left repeat-x;
}

div#page
{
	position: relative;
	padding: 0px;
	margin: auto;
	text-align: left;
	background: url(../images/top_repeat.jpg) top left repeat-x;
	width:780px;
}

div#top
{
	display: block;
	position: relative;
	height: 247px;
	background: url(../images/top.jpg) top left no-repeat;
}

div#left
{
	float: left;
	width: 185px;
	_height:280px;
	min-height:280px;
	margin: 0px 0px 0px 0em;
	padding: 0px 0px 0px 16px;
	background: url(../images/left.jpg) top left repeat;
}

div#right
{
	display:none;
}

div#left_body
{
     padding:0px 0px 50px 0px;
     background: #BFBFBF url(../images/left_body.jpg) bottom left no-repeat;
}

div#right_body
{
    padding:0px 0px 0px 0px;
}

div#menu
{
    position:absolute;
    margin:0px 0px 0px 0px;
    padding:0px 0px px 0px;
    display:block;
    left:0px;
    top:196px;    
    background: url(../images/menu.jpg) bottom left no-repeat;
}

div#menu .box
{
	background: url(../images/menu_repeat.jpg) bottom left repeat-y;
	border:0px;
	padding:0px;
	margin:0px;
}

div#menu_sub
{
	display:block;
	margin:0px 0px 0px 0px;
	padding:0px 0px 0px 0px;
	background: url(../images/menu_sub.jpg) bottom left no-repeat;
}

div#menu_sub .box
{
    padding:0px 0px 0px 0px;
	background: url(../images/menu_sub_repeat.jpg) bottom left repeat-y;
}

div#action_panel
{
	display: none;
}

div#main
{
	background: url(../images/main_repeat.jpg) top left repeat-y;
}

div#main_content
{
	_height:330px;
	min-height:330px;
	padding:0px 0px 0px 0px;
	background: url(../images/main.jpg) top left no-repeat;	
}

div#content_body
{
	margin: 0px 0px 0px 190px;
	background: url(../images/content.jpg) top right no-repeat;
}

div#content_text
{ 
	_height:100px;
